{
  "name": "youtube-in-vscode",
  "displayName": "Youtube in vscode",
  "description": "Search youtube videos while coding in vscode",
  "publisher": "gtjman",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/gtjman/youtube-in-vscode"
  },
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [ 
    "onView:youtube-in-vscode.youtubeSearchWebView"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "youtube-search",
          "title": "YouTube Search",
          "icon": "icon.png"
        }
      ]
    },
    "views": {
      "youtube-search": [
        {
          "type": "webview",
          "id": "youtube-in-vscode.youtubeSearchWebView",
          "name": "YouTube Search"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.69.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  }
}
